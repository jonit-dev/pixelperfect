{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "bootstrap": "yarn install && yarn setup",
    "setup": "chmod +x ./scripts/setup.sh ./scripts/setup/*.sh && ./scripts/setup.sh",
    "setup:env": "./scripts/setup/02-environment.sh",
    "setup:db": "./scripts/setup/05-migrations.sh",
    "setup:db:manual": "./scripts/setup/05-migrations.sh manual",
    "setup:types": "./scripts/setup/06-types.sh",
    "dev": "concurrently -n next,wrangler,stripe -c cyan,magenta,yellow \"next dev\" \"wrangler pages dev --proxy 3000\" \"stripe listen --forward-to localhost:3000/api/webhooks/stripe\"",
    "dev:test": "./scripts/dev-test.sh",
    "build": "next build",
    "start": "next start",
    "pages:build": "npx @cloudflare/next-on-pages",
    "pages:preview": "npm run pages:build && wrangler pages dev .vercel/output/static",
    "deploy": "npm run pages:build && wrangler pages deploy .vercel/output/static",
    "format": "prettier --write --log-level silent \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx} --fix",
    "tsc": "tsc --noEmit",
    "prepare": "husky",
    "verify": "npm run tsc && npm run lint",
    "test:e2e": "playwright test --project=chromium",
    "test:e2e:full": "playwright test",
    "test:api": "playwright test --project=api",
    "test:api:dev": "playwright test --project=api --ui",
    "test:preview": "playwright test --project=workers-preview",
    "test:all": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@amplitude/analytics-browser": "^2.31.3",
    "@baselime/edge-logger": "^0.4.2",
    "@baselime/react-rum": "^0.3.1",
    "@google/genai": "^1.30.0",
    "@google/generative-ai": "^0.24.1",
    "@hookform/resolvers": "^3.9.1",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.46.1",
    "@tremor/react": "^3.14.1",
    "@typescript-eslint/eslint-plugin": "^8.12.2",
    "@typescript-eslint/parser": "^8.12.2",
    "gray-matter": "^4.0.3",
    "jszip": "^3.10.1",
    "lucide-react": "^0.344.0",
    "next": "15.5.2",
    "next-mdx-remote": "^5.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.1",
    "react-icons": "^5.3.0",
    "reading-time": "^1.5.0",
    "recharts": "^2.12.2",
    "stripe": "^20.0.0",
    "zod": "^3.23.8",
    "zustand": "^5.0.1"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.16",
    "@eslint/js": "^9.13.0",
    "@playwright/test": "^1.42.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/ui": "^4.0.13",
    "autoprefixer": "^10.4.18",
    "concurrently": "^9.2.1",
    "daisyui": "^5.5.5",
    "dotenv": "^17.2.3",
    "eslint": "^9.9.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "globals": "^15.9.0",
    "husky": "^9.0.0",
    "jsdom": "^27.2.0",
    "lint-staged": "^16.2.7",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vercel": "^48.10.6",
    "vitest": "^4.0.13",
    "wrangler": "^4.50.0"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e",
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  }
}
