{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "bootstrap": "yarn install && yarn setup",
    "setup": "chmod +x ./scripts/setup.sh ./scripts/setup/*.sh && ./scripts/setup.sh",
    "setup:env": "./scripts/setup/02-environment.sh",
    "setup:db": "./scripts/setup/05-migrations.sh",
    "setup:db:manual": "./scripts/setup/05-migrations.sh manual",
    "setup:types": "./scripts/setup/06-types.sh",
    "dev": "bash -c 'source ./scripts/load-env.sh && concurrently -n next,stripe -c cyan,yellow \"next dev\" \"stripe listen --api-key $STRIPE_SECRET_KEY --forward-to localhost:3000/api/webhooks/stripe\"'",
    "dev:no-webhooks": "next dev",
    "dev:test": "./scripts/dev-test.sh",
    "prebuild": "tsx scripts/build-blog.ts",
    "build": "next build",
    "start": "next start",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "deploy": "./scripts/deploy/deploy.sh",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "format": "prettier --write --log-level silent \"{client,server,shared,app}/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix",
    "tsc": "tsc --noEmit",
    "prepare": "husky",
    "verify": "npm run tsc && npm run lint",
    "verify:full": "npm run verify && npm run test:unit && npm run test:e2e:full",
    "test:e2e": "playwright test --project=chromium",
    "test:e2e:full": "playwright test",
    "test:api": "playwright test --project=api",
    "test:integration": "playwright test --project=integration",
    "test:api:dev": "playwright test --project=api --ui",
    "test:preview": "playwright test --project=workers-preview",
    "test:all": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test": "yarn test:api && yarn test:e2e && vitest run",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "stripe:listen": "bash -c 'source ./scripts/load-env.sh && stripe listen --api-key $STRIPE_SECRET_KEY --forward-to localhost:3000/api/webhooks/stripe'",
    "stripe:setup": "bash -c 'source ./scripts/load-env.sh && npx tsx scripts/setup-stripe.ts'",
    "stripe:setup:test": "bash -c 'source ./scripts/load-env.sh && npx tsx scripts/setup-stripe.ts --test'",
    "stripe:setup:prod": "bash -c 'source ./scripts/load-env.sh && npx tsx scripts/setup-stripe.ts --prod'",
    "stripe:check": "./scripts/load-env.sh --print",
    "i18n:check": "tsx scripts/check-translations.ts",
    "i18n:helper": "tsx scripts/translation-helper.ts",
    "i18n:stats": "tsx scripts/translation-helper.ts stats",
    "i18n:diff": "tsx scripts/translation-helper.ts diff",
    "i18n:batch": "tsx scripts/translation-helper.ts get-batch"
  },
  "dependencies": {
    "@amplitude/analytics-browser": "^2.31.3",
    "@baselime/edge-logger": "^0.4.2",
    "@baselime/react-rum": "^0.3.1",
    "@google/genai": "^1.30.0",
    "@hookform/resolvers": "^3.9.1",
    "@imgly/background-removal": "^1.7.0",
    "@mdx-js/react": "^3.1.0",
    "@stripe/react-stripe-js": "^5.4.1",
    "@stripe/stripe-js": "^8.5.3",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.46.1",
    "@tailwindcss/typography": "^0.5.19",
    "@types/dompurify": "^3.0.5",
    "@typescript-eslint/eslint-plugin": "^8.12.2",
    "@typescript-eslint/parser": "^8.12.2",
    "country-flag-icons": "^1.6.4",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.19",
    "dompurify": "^3.3.1",
    "framer-motion": "^12.23.25",
    "gray-matter": "^4.0.3",
    "jszip": "^3.10.1",
    "lucide-react": "^0.344.0",
    "marked": "^17.0.1",
    "next": "16.0.10",
    "next-intl": "^4.7.0",
    "next-mdx-remote": "5.0.0",
    "onnxruntime-web": "1.21.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.1",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "reading-time": "^1.5.0",
    "remark-gfm": "^4.0.1",
    "replicate": "^1.4.0",
    "stripe": "^20.0.0",
    "zod": "^3.23.8",
    "zustand": "^5.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@opennextjs/cloudflare": "^1.0.0",
    "@playwright/test": "^1.57.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/mdx": "^2.0.13",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/ui": "^4.0.13",
    "autoprefixer": "^10.4.18",
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.3",
    "esbuild": "0.25.4",
    "eslint": "^9.9.1",
    "eslint-plugin-i18next": "^6.1.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "globals": "^15.9.0",
    "husky": "^9.0.0",
    "jsdom": "^27.2.0",
    "lint-staged": "^16.2.7",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vercel": "^50.1.0",
    "vitest": "^4.0.13",
    "wrangler": "^4.50.0"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e",
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Safari versions",
    "last 2 Edge versions",
    "not dead",
    "not IE 11"
  ]
}
